<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.mobileapp.devtools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name="com.project.mobileapp.devtools.AppController"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.project.mobileapp.devtools.Splash"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.project.mobileapp.devtools.Home"
            android:screenOrientation="portrait"
            />
        <activity android:name="com.project.mobileapp.devtools.ListofArticles"
            android:screenOrientation="portrait"
            />
        <activity
            android:screenOrientation="portrait"
            android:name="com.project.mobileapp.devtools.ArticleDescription">
        </activity>
    </application>
</manifest>